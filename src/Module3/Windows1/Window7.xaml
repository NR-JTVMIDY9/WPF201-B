<Window x:Class="Windows1.Window7"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Windows1"
        mc:Ignorable="d"
        Title="Window7" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="btnOK" Click="BtnOK_Click" IsDefault="True" Content="OK" />
            <Button x:Name="btnCancel" IsCancel="True" Content="Cancel" />
        </StackPanel>        
    </Grid>
</Window>
